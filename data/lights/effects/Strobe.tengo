// Strobe flashes all LEDS for a really short time
export {
    info: {
        maxtick: 256,
        frametime: 0.04 // [s]
    },
    frame: func(leds, tick){
        active := tick % 2 == 0
        for group in leds {
            for idx:=0; idx<group.count; idx++ {
                group.brightness[idx] = active ? 1.0 : 0.0
                group.color[idx] = 0
            }
        }
        return leds
    }
}